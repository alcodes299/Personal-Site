<h1>webdesign</h1>
<div id="time">
  <script type="text/javascript">
    var time = document.getElementById("time")
    console.log(time)
    function clock() {
      var today = new Date()
      var h = today.getHours()
      var m = today.getMinutes()
      var s = today.getSeconds()
      m = checkZeros(m)
      s = checkZeros(s)
      time.innerHTML = h+':'+m+':'+s
    }
    function checkZeros(i) {
      if (i < 10) {i ="0" + i}
      return i
    }
    setInterval(clock,1000)

  </script>

</div>
