<script>
  var smallBox = document.getElementById("small-box")
  var t = setInterval(move, 100)
  var pos = 0
  function move(){
    if(pos>175){
      clearInterval(t)
    } else{
      pos += 4
      smallBox.style.left = pos+'px'
    }

  }
  smallBox.addEventListener("click", back)
  function back(){
    function add(){
      if(pos<0){
        clearInterval(y)
      } else {
        pos-=4
        smallBox.style.left = pos+'px'
      }
    }
    var y = setInterval(add,100)
  }

</script>
